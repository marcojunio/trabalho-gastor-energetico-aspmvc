@model GestorEnergetico.ViewModel.ParametroViewModel

@{
    ViewData["Title"] = "Detalhes";
}

<h1>Detalhes</h1>

<div>
    <h4>Detalhes do Parametro</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => modelItem.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ValorKwh)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => modelItem.ValorKwh)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FaixaConsumoAlto)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => modelItem.FaixaConsumoAlto)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FaixaConsumoMedio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => modelItem.FaixaConsumoMedio)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Voltar</a>
</div>